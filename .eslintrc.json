{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "no-underscore-dangle": 0,
        "consistent-return": 0,
        "no-param-reassign": 0,
        "no-unused-vars": ["error", { "argsIgnorePattern": "next" }]
    }
}